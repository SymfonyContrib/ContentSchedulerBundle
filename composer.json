{
    "name": "symfonycontrib/content-scheduler-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle for scheduling tasks on content to be completed. (ex. scheduled (un)publishing)",
    "keywords": ["Symfony", "bundle", "publishing"],
    "homepage": "http://symfonycontrib.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Alan Doucette",
            "email": "dragonwize@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4",
        "symfony/symfony": "~2.3"
    },
    "autoload": {
        "psr-0": {
            "SymfonyContrib\\Bundle\\ContentSchedulerBundle\\": ""
        }
    },
    "target-dir": "SymfonyContrib/Bundle/ContentSchedulerBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
